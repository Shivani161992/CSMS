  <LeftMargin>0.5in</LeftMargin>
  <DataSets>
    <DataSet Name="CSIC">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select  dbo.tbl_MetaData_DEPOT.DepotName,tbl_MetaData_DEPOT.DepotID,tbl_MetaData_STORAGE_COMMODITY . Commodity_Name,issue_opening_balance.Commodity_Id,districtsmp .district_name,tbl_MetaData_DCP_S.Scheme_Name, issue_opening_balance.Quantity, Sum

(issue_opening_balance.Current_Balance)   as Balance,issue_opening_balance.Month  from dbo.tbl_MetaData_DEPOT

left Join dbo.issue_opening_balance on tbl_MetaData_DEPOT .DepotID= issue_opening_balance.Depotid  

left join dbo.tbl_MetaData_STORAGE_COMMODITY on tbl_MetaData_STORAGE_COMMODITY . Commodity_Id=issue_opening_balance.Commodity_Id

left join dbo.tbl_MetaData_DCP_S on tbl_MetaData_DCP_S.Scheme_Id=issue_opening_balance.Scheme_Id

left join pds.districtsmp on districtsmp .district_code=Substring(@DistID,3,2)


where  tbl_MetaData_DEPOT.DepotID=@DepotID

group by tbl_MetaData_DEPOT.DepotName,tbl_MetaData_STORAGE_COMMODITY . Commodity_Name,tbl_MetaData_DCP_S.Scheme_Name,districtsmp .district_name,issue_opening_balance.Quantity,issue_opening_balance.Commodity_Id,issue_opening_balance.Month,tbl_MetaData_DEPOT.DepotID

order by issue_opening_balance.Month</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DistID">
            <Value>=Parameters!DistID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DepotID">
            <Value>=Parameters!DepotID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>dsform13</DataSourceName>
      </Query>
