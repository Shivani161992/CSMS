    <DataSet Name="ds_drdist">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT            tbl_MetaData_DEPOT.DepotName, tbl_Receipt_Details.Dist_Id, tbl_Receipt_Details.Depot_ID,tbl_Receipt_Details.S_of_arrival, Source_Arrival_Type.Source_Name, DepoCode.DepoName, 
                             tbl_Receipt_Details.arrival_date, tbl_Receipt_Details.challan_no, tbl_Receipt_Details.Qty, tbl_Receipt_Details.Commodity, 
                             tbl_Receipt_Details.Scheme, tbl_Receipt_Details.Vehile_no, tbl_Receipt_Details.Arrival_time, 
                             tbl_Receipt_Details.No_of_Bags, tbl_Receipt_Details.Recd_Qty, tbl_Receipt_Details.Recieved_Bags, 
                             tbl_Receipt_Details.Variation_qty, tbl_Receipt_Details.A_dist ,tbl_MetaData_STORAGE_COMMODITY .Commodity_Name,tbl_MetaData_DCP_S.Scheme_Name ,DepotMP.DepotName ,DistMP.district_name as Distmpname 

FROM   dbo.tbl_MetaData_DEPOT
left join   tbl_Receipt_Details on  tbl_MetaData_DEPOT .DepotID=  tbl_Receipt_Details.Depot_ID and  tbl_Receipt_Details.S_of_arrival=03  and  tbl_Receipt_Details.arrival_date&gt;=@fromDate and  tbl_Receipt_Details.arrival_date&lt;=@toDate
 Left JOIN
                             Source_Arrival_Type ON tbl_Receipt_Details.S_of_arrival = Source_Arrival_Type.Source_ID Left JOIN
                             DepoCode ON tbl_Receipt_Details.A_dist = DepoCode.District_Code AND 
                             tbl_Receipt_Details.A_Depo = DepoCode.DepoCode 
 left join tbl_MetaData_STORAGE_COMMODITY on tbl_Receipt_Details.Commodity=tbl_MetaData_STORAGE_COMMODITY .Commodity_ID
left join dbo.tbl_MetaData_DCP_S on  tbl_Receipt_Details.Scheme=tbl_MetaData_DCP_S.Scheme_ID
left join pds.districtsmp as DistMP on tbl_Receipt_Details.Dist_Id=DistMP.district_code
left join dbo.tbl_MetaData_DEPOT as DepotMP on tbl_Receipt_Details.Depot_ID=DepotMP.DepotID
where   tbl_MetaData_DEPOT.DistrictId=@Dist</CommandText>
        <QueryParameters>
          <QueryParameter Name="@fromDate">
            <Value>=Parameters!fromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@toDate">
            <Value>=Parameters!toDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dist">
            <Value>=Parameters!Dist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>dsform13</DataSourceName>
      </Query>
